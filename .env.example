# Copy this file to .env.local and fill in your actual values

# Xano Database Configuration
XANO_API_URL=https://your-instance.xano.io/api:your-version
XANO_API_KEY=your-xano-api-key

# Thirdweb Configuration
NEXT_PUBLIC_THIRDWEB_CLIENT_ID=client-id
THIRDWEB_SECRET_KEY=secret-key
THIRDWEB_WEBHOOK_SECRET=webhook-secret

# NextAuth Configuration (for future stories)
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=random-secret

# Environment
NODE_ENV=development